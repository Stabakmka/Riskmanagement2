VIEW "COM_ORGANON_TOCS_V_MARC"
    AS SELECT 
    "COM_ORGANON_TOCS_VT_MARA"."MATNR",
    "WERKS",
    "ZZMAT_INHERITREL",
    "ZZMAT_SENS_TYPE",
    "ZZMAT_MAXTIMEDAY",
    "ZZMAT_MAXTIMEHRS",
    "ZZMAT_MAXTIMEMIN",
    "ZZMAT_WARNTIMEDAY",
    "ZZMAT_WARNTIMEHRS",
    "ZZMAT_WARNTIMEMNS",
    "COM_ORGANON_TOCS_VT_MARA"."MANDT"
    FROM "COM_ORGANON_TOCS_VT_MARA"
    INNER JOIN "COM_ORGANON_TOCS_VT_MARC"
    ON "COM_ORGANON_TOCS_VT_MARA"."MATNR" = "COM_ORGANON_TOCS_VT_MARC"."MATNR"
    AND "COM_ORGANON_TOCS_VT_MARA"."MANDT" = "COM_ORGANON_TOCS_VT_MARC"."MANDT"